{
  "plugins": [
    "react-native-share",
    {
      "ios": [
        "fb",
        "instagram",
        "twitter",
        "tiktoksharesdk",
      ],
      "android": [
        "com.facebook.katana",
        "com.instagram.android",
        "com.twitter.android",
        "com.zhiliaoapp.musically",
      ],
      "enableBase64ShareAndroid": true
    }
  ],
  "expo": {
    "name": "Pensieve",
    "slug": "pensieve",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icons/adaptive-icon.png",
    "scheme": "pensieve",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pensieve.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pensieve.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon-light.png",
          "dark": {
            "image": "./assets/images/icons/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-document-picker",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b0526d2c-3052-4dd1-b08c-3043cb463267"
      }
    }
  }
}
